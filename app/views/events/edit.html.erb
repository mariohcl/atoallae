<section class="wrap-edit-profile">
  <section class="wrap-edit-profile-center">

    <h1>Editar Evento</h1>

    <div id="tabs-container">

        <ul class="tabs-menu">
            <li class="current"><a href="#tab-1">Info de tu Evento</a></li>
        </ul>

        <div class="tab">

            <div id="tab-1" class="tab-content">

            <%= render 'form', event: @event %>

            <nav class="access-form">
              <ul>
                <li><%= link_to 'Ver Evento', page_event_path(@page, @event) %></li>
              </ul>
            </nav>

            </div>

        </div>
    </div>

  </section>
</section>
